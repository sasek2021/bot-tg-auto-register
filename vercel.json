{
  "version": 2,
  "builds": [
    {
      "src": "bot.py",
      "use": "@vercel/python"
    },
    {
      "src": "proxy_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "bot.py"
    },
    {
      "src": "/register",
      "dest": "proxy_server.py"
    }
  ]
}
