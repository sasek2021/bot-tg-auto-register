{
  "version": 2,
  "builds": [
    {
      "src": "proxy_server.py",
      "use": "@vercel/python"
    },
    {
      "src": "bot.py",
      "use": "@vercel/python"
    },
    {
      "src": "setup_webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/register",
      "dest": "/proxy_server.py"
    },
    {
      "src": "/bot",
      "dest": "/bot.py"
    },
    {
      "src": "/webhook",
      "dest": "/setup_webhook.py"
    }
  ]
}
